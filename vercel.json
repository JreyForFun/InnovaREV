{
  "rewrites": [
    {
      "source": "/portfolio_templates/:path*",
      "destination": "/portfolio_templates/:path*"
    },
    {
      "source": "/resto_templates/:path*",
      "destination": "/resto_templates/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/public/:path*"
    }
  ]
}